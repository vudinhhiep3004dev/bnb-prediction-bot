{
  "name": "bnb-prediction-bot",
  "version": "2.0.0",
  "description": "Telegram bot for BNB price prediction with enhanced accuracy (60-75%) using weighted scoring system, 13 technical indicators, and advanced order flow analysis for PancakeSwap Prediction game",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test:binance": "tsx scripts/test-binance.ts",
    "test:ai": "tsx scripts/test-ai.ts",
    "test:indicators": "tsx scripts/test-indicators.ts",
    "test:enhanced": "tsx scripts/test-enhanced-indicators.ts",
    "test:price": "tsx scripts/test-price-prediction.ts",
    "test:hybrid": "tsx scripts/test-hybrid-price.ts",
    "test:all": "bun run test:binance && bun run test:indicators && bun run test:enhanced && bun run test:price && bun run test:hybrid && bun run test:ai",
    "backtest": "tsx src/scripts/run-backtest.ts"
  },
  "keywords": [
    "telegram",
    "bot",
    "bnb",
    "prediction",
    "pancakeswap",
    "ai",
    "gemini"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "dotenv": "^17.2.3",
    "ethers": "^6.15.0",
    "telegraf": "^4.16.3",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "prettier": "^3.6.2",
    "eslint": "^9.36.0"
  }
}
